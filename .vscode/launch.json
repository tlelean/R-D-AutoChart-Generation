{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Run Tests Mode",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/DLS Chart Generation/main.py",
      "args": ["--run-tests"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Python: Generate PDF",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/DLS Chart Generation/main.py",
      "args": ["${workspaceFolder}/Example Data/Mass Spec/_Data_3-9-2025_15-24-3.csv", "${workspaceFolder}/Example Data/Mass Spec/_Test_Details_3-9-2025_15-24-3.csv", "${workspaceFolder}/Example Data/Mass Spec"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}"
    }
  ]
}



